{
	"compilerOptions": {
		"outDir": "./dist/",
		"module": "commonjs",
		"target": "ES2020",
		"sourceMap": true,
		"jsx": "react-jsx",
		"allowJs": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"isolatedModules": true
	}
}
