{
  "name": "joplin-plugin-anki-sync",
  "version": "0.0.2-next.0",
  "scripts": {
    "dist": "webpack --joplin-plugin-config buildMain && webpack --joplin-plugin-config buildExtraScripts && webpack --joplin-plugin-config createArchive",
    "prepare": "npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "version": "node scripts/update-manifest.js",
    "build": "yarn run dist",
    "dev": "concurrently --raw -n main,webview,content \"webpack --joplin-plugin-config buildMain --progress --watch\" \"webpack --joplin-plugin-config buildWebviewScripts --progress --watch\" \"webpack --joplin-plugin-config buildExtraScripts --progress --watch\""
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^4.2.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@autoanki/core": "0.0.2-next.1",
    "@material-ui/core": "^4.11.4",
    "@types/markdown-it": "^12.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.1.0"
  }
}
